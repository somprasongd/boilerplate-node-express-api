    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist --copy-files",
    "prod": "export NODE_ENV=production||set NODE_ENV=production&&npm run build&&node dist/index.js",
    "start": "node dist/index.js",
    "lint": "eslint src/ --fix"
  },
