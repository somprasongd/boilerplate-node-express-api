    "compression": "^1.7.4",
    "config": "^3.3.8",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "express-graceful-shutdown": "^1.0.0",
    "handle-promise": "^1.0.0",
    "helmet": "^3.21.2",
    "joi": "^13.4.0",
    "joi-objectid": "^2.0.0",
